{
    "name": "mariojgt/onixpro",
    "description": "A Laravel Package Onixpro, a quick start for new laravel Packages",
    "keywords": [
        "laravel",
        "grapejs",
        "grape js",
        "ck editor",
        "laravel page builder",
        "page builder",
        "builder"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "mario",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require":{
        "php": "^7.2|^8.0",
        "mariojgt/onix": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Mariojgt\\Onixpro\\": "src"
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "Mariojgt\\Onixpro\\OnixproProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
